{
  "name": "@maneko/prettier-config",
  "type": "module",
  "version": "0.1.2",
  "packageManager": "pnpm@10.18.3",
  "description": "Maneko Group's Prettier config",
  "author": "Maneko Group",
  "license": "MIT",
  "homepage": "https://github.com/maneko-group/prettier-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maneko-group/prettier-config.git"
  },
  "bugs": {
    "url": "https://github.com/maneko-group/prettier-config/issues"
  },
  "keywords": [
    "prettier-config"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./index.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.d.ts",
    "index.js"
  ],
  "scripts": {
    "format": "prettier --write \"*.js\"",
    "prepare": "husky"
  },
  "peerDependencies": {
    "prettier": "^3.8.1"
  },
  "devDependencies": {
    "bumpp": "^10.3.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.4",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write"
    ]
  }
}
